{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["./stubs"]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "ConnexCS Programmable Voice Documentation",
        "disableSort": false,
        "collapse": true,
        "resources": {
            "google": "https://docs.connexcs.com/"
        }
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true
    }
}